{
  "name": "page-screenshot",
  "version": "1.0.0",
  "description": "网页屏幕截图",
  "main": "lib/page-screenshot.js",
  "types": "types/page-screenshot.d.ts",
  "files": [
    "index.js",
    "dist",
    "types",
    "src"
  ],
  "scripts": {
    "dev": "webpack-dev-server --config webpack.dev.config.js",
